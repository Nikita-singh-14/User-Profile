              <Link to="/" className="text-3xl font-bold text-white tracking-widest hover:text-yellow-400 transition">
                  Dream<span className="text-yellow-400">Adv</span>
              </Link>
              
              {/* Desktop Menu */}
              <nav className="hidden md:flex space-x-6 text-white text-lg font-medium">
                  {/* Use Link for all internal navigation */}
                  <Link to="/" className="hover:text-yellow-400 transition">Home</Link>
                  
                  
